
package jp.co.MarutouCompack.commonClass.PrinterDat;

import com.google.gson.annotations.SerializedName;

/**
 * ガス料金ステップデータクラス.
 */
public class GstpDat {
    /** 上限値 */
    @SerializedName("m_nUpLimit")
    private int mUplimit;
    /** 加算値 */
    @SerializedName("m_nAddp")
    private int mAdd;
    /** 基準料金 */
    @SerializedName("m_nBase")
    private int mBase;

    public GstpDat() {
    }

    public GstpDat(int mUplimit, int mAdd, int mBase) {
        this.mUplimit = mUplimit;
        this.mAdd = mAdd;
        this.mBase = mBase;
    }

    public int getUplimit() {
        return mUplimit;
    }

    public void setUplimit(int mUplimit) {
        this.mUplimit = mUplimit;
    }

    public int getAdd() {
        return mAdd;
    }

    public void setAdd(int mAdd) {
        this.mAdd = mAdd;
    }

    public int getBase() {
        return mBase;
    }

    public void setBase(int mBase) {
        this.mBase = mBase;
    }
}